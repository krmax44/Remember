{
	"manifest_version": 2,
	"name": "Remember",
	"description": "Remember saves your snippets",
	"version": "1.1",
	"browser_action": {
	    "default_icon": "img/icon-128.png",
	    "default_popup": "popup.html"
	},
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},
	"permissions": ["storage", "unlimitedStorage"]
}